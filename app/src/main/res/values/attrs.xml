<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="TimeSelectorView">
        <attr name="type" format="enum" >
            <enum name="duration" value="0"/>
            <enum name="priority" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ButtonView">
        <attr name="android:text" format="string"/>
        <attr name="buttonType" format="enum" >
            <enum name="disable" value="0"/>
            <enum name="active" value="1"/>
        </attr>
    </declare-styleable>

</resources>